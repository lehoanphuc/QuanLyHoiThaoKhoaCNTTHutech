@model Demo1.Models.SANDAU
@{
    ViewBag.Title = "Tạo sân đấu";
}

@using (Html.BeginForm())
{
    <h4 style="color: red">
        @ViewData["CheckSanDau"]
    </h4>
    @Html.AntiForgeryToken()
    <div class="form-group">
        <div class="col-md-10">
            <input placeholder="Tên sân đấu" id="TENSANDAU" type="text" name="TENSANDAU"
                   @Html.EditorFor(model => model.MaSanDau, new { htmlAttributes = new { @class = "form-control" } })
        </div>
    </div>


    <div class="form-group">
        <input type="submit" value="Thêm" class="btn btn-primary" />
    </div>
}


